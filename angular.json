{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "docs-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "indexTransform": "./src/lib/utilities/pre-loader/",
            "outputPath": "dist",
            "index": "src/docs-ui/index.html",
            "main": "src/docs-ui/main.ts",
            "tsConfig": "src/docs-ui/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "aot": false,
            "assets": [
              "src/assets",
              {
                "glob": "*",
                "input": "src/lib/theme/icons/",
                "output": "/assets/icons/"
              },
              {
                "glob": "favicon.ico",
                "input": "src/docs-ui/",
                "output": "/"
              }
            ],
            "styles": ["src/theme.scss", "src/docs-ui/styles.scss"],
            "scripts": ["node_modules/hammerjs/hammer.min.js"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/docs-ui/environments/environment.ts",
                  "with": "src/docs-ui/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "docs-ui:build",
            "proxyConfig": "./proxy.conf.js"
          },
          "configurations": {
            "production": {
              "browserTarget": "docs-ui:build:production",
              "proxyConfig": "./proxy.conf.js"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "docs-ui:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/docs-ui/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/docs-ui/tsconfig.spec.json",
            "scripts": ["node_modules/hammerjs/hammer.min.js"],
            "styles": ["src/theme.scss", "src/docs-ui/styles.scss"],
            "assets": [
              "src/assets",
              {
                "glob": "favicon.ico",
                "input": "src/docs-ui/",
                "output": "/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/docs-ui/tsconfig.app.json", "src/docs-ui/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "docs-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "docs-ui:serve:production"
            }
          }
        }
      }
    },
    "ui-platform": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "src/lib/tsconfig.lib.json",
            "project": "src/lib/ng-package.js"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/lib/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/lib/tsconfig.spec.json",
            "scripts": ["node_modules/hammerjs/hammer.min.js"],
            "styles": ["src/theme.scss"],
            "assets": ["src/assets"]
          }
        }
      }
    }
  },
  "defaultProject": "docs-ui",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "analytics": false
  }
}
